import java.util.Scanner;

class A1Q10_SumOfDigits
{
	public static void main(String args[])
	{
		System.out.print("Enter a number: ");
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		
		int sum = 0;
		while( num != 0)
		{
			sum = sum + (num % 10);
			num = num / 10;
		}
		
		System.out.print("The sum of digits is: " + sum);
	}
}